{
  "compilerOptions": {
    "diagnostics": true,
    "esModuleInterop": true,
    "extendedDiagnostics": true,
    "skipLibCheck": true,
    "noEmit": false,
    "jsx": "preserve",
    "declaration": true,
    "lib": ["dom", "es2020"],
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "target": "es2020"
  },
  "exclude": ["built-types"]
}
